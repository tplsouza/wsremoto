package decisao;

import javax.swing.JOptionPane;

public class DesafioDo {

	public static void main(String[] args) {
		int nro1 = Integer.parseInt(JOptionPane.showInputDialog("digite numero"));
		int nro2 = 0;
		
		do {
			nro2 = Integer.parseInt(JOptionPane.showInputDialog("adivinhe"));
		if (nro1 > nro2) {
			System.out.println("Errou - numéro é maior");
		} else {
			System.out.println("Errou - numéro é menor");
		}

		} while(nro1 != nro2);
		
		
		
		System.out.println("Acertou");
	}

}
